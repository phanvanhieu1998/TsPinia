{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "esModuleInterop": false,
    "types": ["node"],
    "allowImportingTsExtensions": false,
    "skipLibCheck": true
   
  },
  "include": [
    "package.json",
    "src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "src/shims-vue.d.ts"
  ]
}
